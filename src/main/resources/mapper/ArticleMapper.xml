<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arccc.cmsserver.mapper.ArticleMapper">

    <resultMap id="BaseResultMap" type="com.arccc.cmsserver.domain.Article">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="mainTitle" column="main_title" jdbcType="VARCHAR"/>
            <result property="subTitlte" column="sub_titlte" jdbcType="VARCHAR"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="date" column="date" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,main_title,sub_titlte,
        author,date
    </sql>
</mapper>
